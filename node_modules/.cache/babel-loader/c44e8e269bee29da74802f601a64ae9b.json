title\"\n        ></v-text-field>\n        <v-select\n          :items=\"categories\"\n          label=\"Категория\"\n          v-model=\"noteData.category\"\n          :value=\"noteData.category\"\n        ></v-select>\n        <v-textarea\n          name=\"text\"\n          filled\n          label=\"Текст\"\n          auto-grow\n          clearable\n          :value=\"noteData.text\"\n          v-model=\"noteData.text\"\n        ></v-textarea>\n\n        <v-btn class=\"ma-2\" outlined color=\"black\" block type=\"submit\">\n          Изменить запись\n        </v-btn>\n      </v-form>\n    </Modal>\n  </div>\n</template>\n\n<script>\nimport * as database from \"../../store/db\";\nexport default {\n  name: \"NoteCard\",\n  data() {\n    return {\n      showModal: false,\n      categories: [\"Важные\", \"Срочные\", \"На потом\"],\n    };\n  },\n  props: {\n    noteData: Object,\n  },\n  methods: {\n    deleteNote() {\n      database.deleteNote(this.noteData.id).catch((err) => {\n        console.error(\"Error\");\n      });\n    },\n    editNote() {\n      database\n        .editNote(\n          this.noteData.title,\n          this.noteData.category,\n          this.noteData.text,\n          this.noteData.noteId\n        )\n        .catch((err) => {\n          console.error(\"Error\");\n        });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.note-title {\n  background-color: rgba(0, 0, 0, 0.5);\n}\n.v-card__text {\n  height: 150px;\n}\n@media (max-width: 400px) {\n  .v-card {\n    width: 300px;\n}}\n</style>"],"sourceRoot":"src/components/Note"}]}