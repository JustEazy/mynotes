FA;AAGA,MAAA,QAAA,EAAA,EAHA;AAIA,MAAA,SAAA,EAAA,CAAA,UAAA,KAAA;AAAA,eAAA,CAAA,CAAA,KAAA,IAAA,4BAAA;AAAA,OAAA,CAJA;AAKA,MAAA,QAAA,EAAA,KALA;AAMA,MAAA,UAAA,EAAA,CACA,UAAA,KAAA;AAAA,eAAA,CAAA,CAAA,KAAA,IAAA,4BAAA;AAAA,OADA,EAEA,UAAA,KAAA,EAAA;AACA,YAAA,OAAA,GAAA,sJAAA;AACA,eAAA,OAAA,CAAA,IAAA,CAAA,KAAA,KAAA,iBAAA;AACA,OALA,CANA;AAaA,MAAA,aAAA,EAAA,CACA,UAAA,KAAA;AAAA,eAAA,CAAA,KAAA,IAAA,EAAA,EAAA,MAAA,GAAA,CAAA,IAAA,oBAAA;AAAA,OADA,EAEA,UAAA,KAAA;AAAA,eAAA,CAAA,CAAA,KAAA,IAAA,4BAAA;AAAA,OAFA,CAbA;AAiBA,MAAA,OAAA,EAAA;AAjBA,KAAA;AAmBA,GAtBA;AAuBA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AAAA;;AACA,MAAA,QAAA,CACA,IADA,GAEA,8BAFA,CAEA,KAAA,KAFA,EAEA,KAAA,QAFA,EAGA,IAHA,CAGA,UAAA,IAAA,EAAA;AACA,QAAA,IAAA,CAAA,IAAA,CACA,aADA,CACA;AACA,UAAA,WAAA,EAAA,KAAA,CAAA,IADA;AAEA,UAAA,KAAA,EAAA,KAAA,CAAA;AAFA,SADA,EAKA,IALA,CAKA,YAAA;AACA,UAAA,QAAA,CACA,IADA,GAEA,WAFA,CAEA,qBAFA,GAGA,IAHA,CAGA,YAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,YAAA;AACA,WALA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WARA;AASA,SAfA,EAgBA,IAhBA,CAgBA,YAAA;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,SAAA;AACA,SAlBA;AAmBA,OAvBA,EAwBA,KAxBA,CAwBA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,OAAA;AACA,OA1BA;AA2BA,KA7BA;AA8BA,IAAA,gBA9BA,8BA8BA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,OAFA,EAEA,IAFA,CAAA;AAGA;AAnCA;AAvBA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-form class=\"authform\" @submit.prevent=\"submit\">\n      <v-text-field\n        v-model=\"name\"\n        label=\"Имя*\"\n        :rules=\"nameRules\"\n        color=\"#F5F5F5\"\n        clearable\n      ></v-text-field>\n      <v-text-field\n        v-model=\"email\"\n        type=\"email\"\n        label=\"Почта*\"\n        :rules=\"emailRules\"\n        color=\"#F5F5F5\"\n        clearable\n      ></v-text-field>\n      <v-text-field\n        v-model=\"password\"\n        label=\"Пароль*\"\n        :rules=\"passwordRules\"\n        :type=\"showicon ? 'text' : 'password'\"\n        :append-icon=\"showicon ? 'fas fa-eye' : 'fas fa-eye'\"\n        color=\"#F5F5F5\"\n        @click:append=\"showicon = !showicon\"\n      ></v-text-field>\n      <v-btn\n        depressed\n        type=\"submit\"\n        :loading=\"loading\"\n        @click=\"simulateProgress\"\n      >\n        <span>Регистрация</span>\n        <template v-slot:loading>\n          <q-spinner-gears />\n        </template>\n      </v-btn>\n      <router-link\n        to=\"login\"\n        style=\"color: black; text-decoration-color: black; text-align: center\"\n        >Я зарегистрирован. Перейти на страницу авторизации</router-link\n      >\n    </v-form>\n  </div>\n</template>\n<script>\nimport firebase from \"firebase\";\n\nexport default {\n  name: \"Register\",\n  data() {\n    return {\n      email: \"\",\n      name: \"\",\n      password: \"\",\n      nameRules: [(value) => !!value || \"Обязательно для заполнения\"],\n      showicon: false,\n      emailRules: [\n        (value) => !!value || \"Обязательно для заполнения\",\n        (value) => {\n          const pattern = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\n          return pattern.test(value) || \"Неверный формат\";\n        },\n      ],\n      passwordRules: [\n        (value) => (value || \"\").length > 6 || \"Минимум 6 символов\",\n        (value) => !!value || \"Обязательно для заполнения\",\n      ],\n      loading: false,\n    };\n  },\n  methods: {\n    submit() {\n      firebase\n        .auth()\n        .createUserWithEmailAndPassword(this.email, this.password)\n        .then((data) => {\n          data.user\n            .updateProfile({\n              displayName: this.name,\n              email: this.email,\n            })\n            .then(() => {\n              firebase\n                .auth()\n                .currentUser.sendEmailVerification()\n                .then(function () {\n                  console.log(\"registered\");\n                })\n                .catch(function (error) {\n                  console.log(\"error\");\n                });\n            })\n            .then(() => {\n              this.$router.push(\"/verify\");\n            });\n        })\n        .catch((err) => {\n          this.error = err.message;\n        });\n    },\n    simulateProgress() {\n      this.loading = true;\n      setTimeout(() => {\n        this.loading = false;\n      }, 3000);\n    },\n  },\n};\n</script>\n<style scoped>\n.v-text-field {\n  font-size: 16pt;\n}\n.authform {\n  padding: 30px;\n}\n.v-btn {\n  width: 100%;\n  margin-top: 20px;\n  margin-bottom: 10px;\n}\n.v-btn span {\n  margin-right: 5px;\n}\n@media (max-width: 1025px) {\n  .v-text-field {\n    font-size: 13pt;\n  }\n}\n</style>"],"sourceRoot":"src/components/Auth"}]}