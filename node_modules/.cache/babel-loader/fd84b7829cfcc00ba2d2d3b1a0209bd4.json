AGA,YAAA;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CAAA,YAAA;AACA,OALA;AAMA,KARA;AASA,IAAA,UATA,wBASA;AACA,MAAA,QAAA,CACA,UADA,CACA,KAAA,cAAA,CAAA,KADA,EACA,KAAA,cAAA,CAAA,IADA,EAEA,KAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,OAAA;AACA,OAJA;AAKA;AAfA,GAnBA;AAoCA,EAAA,OAAA,EAAA,mBAAA;AAAA;;AACA,IAAA,WAAA,CAAA,YAAA;AACA,MAAA,MAAA,CAAA,OAAA,GAAA,MAAA,GAAA,MAAA,CAAA,KAAA,CAAA;AACA,KAFA,EAEA,IAFA,CAAA;AAGA;AAxCA,CAAA","sourcesContent":["<template>\n  <v-app id=\"inspire\">\n    <v-navigation-drawer v-model=\"drawer\" app absolute temporary width=\"300px\">\n      <v-list-item>\n        <v-list-item-content>\n          <v-list-item-title class=\"title\">\n            {{ user.data.displayName }}\n          </v-list-item-title>\n          <v-list-item-subtitle>\n            <b>{{ rtClock }}</b>\n          </v-list-item-subtitle>\n          <v-list-item-subtitle>\n            {{ user.data.email }}\n          </v-list-item-subtitle>\n        </v-list-item-content>\n      </v-list-item>\n\n      <v-divider></v-divider>\n\n      <v-list>\n        <v-list-item link @click=\"showModal = true\">\n          <v-list-item-icon>\n            <v-icon>fas fa-edit</v-icon>\n          </v-list-item-icon>\n          <v-list-item-content>\n            <v-list-item-title>Добавить запись</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n        <v-list-item link @click.prevent=\"signOut\">\n          <v-list-item-icon>\n            <v-icon>fas fa-sign-out-alt</v-icon>\n          </v-list-item-icon>\n          <v-list-item-content>\n            <v-list-item-title>Выйти из учетной записи</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item> </v-list\n      ><Modal v-model=\"showModal\" title=\"Добавить запись\">\n        <v-form @submit.prevent=\"addNewNote\">\n          <v-text-field\n            label=\"Заголовок\"\n            name=\"title\"\n            v-model=\"addNewNoteData.title\"\n          ></v-text-field>\n          <v-textarea\n            name=\"text\"\n            filled\n            label=\"Текст\"\n            auto-grow\n            clearable\n            v-model=\"addNewNoteData.text\"\n          ></v-textarea>\n          <v-btn class=\"ma-2\" outlined color=\"black\" block type=\"submit\">\n            Добавить запись\n          </v-btn>\n        </v-form>\n      </Modal>\n    </v-navigation-drawer>\n\n    <v-app-bar\n      app\n      shrink-on-scroll\n      src=\"https://picsum.photos/id/1082/1920/300\"\n      height=\"150\"\n    >\n      <v-app-bar-nav-icon\n        @click.stop=\"drawer = !drawer\"\n        class=\"white\"\n      ></v-app-bar-nav-icon>\n\n      <v-toolbar-title>Записи</v-toolbar-title>\n      <template v-slot:extension>\n        <v-tabs v-model=\"tab\" align-with-title>\n          <v-tabs-slider color=\"white\"></v-tabs-slider>\n\n          <v-tab v-for=\"item in items\" :key=\"item\">\n            {{ item }}\n          </v-tab>\n        </v-tabs>\n      </template>\n    </v-app-bar>\n\n    <v-main>\n      <v-tabs-items v-model=\"tab\">\n        <v-tab-item class=\"note-tab\"\n          ><note-card\n            v-for=\"(note, index) in notes\"\n            :key=\"index\"\n            :noteData=\"note\"\n            class=\"note-card\"\n        /></v-tab-item>\n      </v-tabs-items>\n    </v-main>\n  </v-app>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\nimport firebase from \"firebase\";\nimport NoteCard from \"../components/Note/NoteCard.vue\";\nimport moment from \"moment\";\nimport * as database from \"../store/db\";\n\nexport default {\n  data: () => ({\n    drawer: false,\n    tab: null,\n    items: [\"Все\", \"Важные\", \"Срочные\", \"На потом\"],\n    rtClock: moment().format(\"LTS\"),\n    showModal: false,\n    addNewNoteData: {\n      title: \"\",\n      text: \"\",\n    },\n  }),\n  components: { NoteCard },\n  computed: {\n    ...mapGetters({\n      user: \"user\",\n      notes: \"notes\",\n    }),\n  },\n  methods: {\n    signOut() {\n      firebase\n        .auth()\n        .signOut()\n        .then(() => {\n          this.$router.replace(\"auth/login\");\n        });\n    },\n    addNewNote() {\n      database\n        .addNewNote(this.addNewNoteData.title, this.addNewNoteData.text)\n        .catch((err) => {\n          console.error(\"Error\");\n        });\n    },\n  },\n  mounted: function () {\n    setInterval(() => {\n      this.rtClock = moment().format(\"LTS\");\n    }, 1000);\n  },\n};\n</script>\n<style>\n.vm-btn-close,\n.vm-btn-close:active,\n.vm-btn-close:focus {\n  outline: none !important;\n}\n.note-tab {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-around;\n  align-items: center;\n  flex-wrap: wrap;\n  margin: 10px;\n}\n.note-card {\n  margin-bottom: 10px;\n}\n</style>"],"sourceRoot":"src/views"}]}